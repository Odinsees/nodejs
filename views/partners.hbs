<h1>PARTNERS</h1>
{{#if partners.length}}
  {{#each partners}}
    <div class="row">
      <div class="col s6 offset-s3">
        <div class="card">
          <div class="card-image">
            <img src="{{logoUrl}}" alt={{name}}/>
          </div>
          <div class="card-content">
            <span class="card-title">{{name}}</span>
            <p class="partnerName">{{number}}</p>
          </div>
          <div class="card-action">
            <a href="/partners/{{id}}">About this partner</a>
          </div>
        </div>
      </div>
    </div>
  {{/each}}
{{else}}
  <p>Partners in process</p>
{{/if}}